import mods.modularmachinery.RecipeBuilder;
import mods.modularmachinery.RecipePrimer;
##导入MM包

RecipeBuilder.newBuilder("F1", "fluid_processor",40).
addItemInput(<mekanism:dust:4>*1).
addFluidInput(<liquid:liquidlithium>*10).
addFluidInput(<liquid:sulfuricacid>*10).
addEnergyPerTickInput(50).
addFluidOutput(<liquid:electrolyte>*20).setChance(0.75).
build();
##银粉加硫酸加液态锂混合得出电解液

RecipeBuilder.newBuilder("F2", "fluid_processor",100).
addItemInput(<cell_empty>*1).
addItemInput(<pneumaticcraft:air_canister>*1).
addEnergyperTickInput(100).
addItemOutput(<cell_cold>*1).setChance(0.8).
addItemOutput(<pneumaticcraft:air_canister:30000>*1).
bulid();
##空单元加充满的空气罐丢入液体处理机有0.8的概率获得一个液氮单元（空单元有概率损坏）

RecipeBuilder.newBuilder("m1", "Mixing_tank",6000).
addItemInput(<minecraft:glowstone_dust>*1).
addFluidInput(<liquid:sulfuricacid>*1250).
addEnergyperTickInput(500).
addItemOutput(<crafttweaker:High_purity_calcium_carbonate>*2).
addFluidOutput(<liquid:HF>*1000).
build();
##使用萤石以及硫酸在mixing_tank中制取氟化氢（高纯碳酸钙为副产物）

RecipeBuilder.newBuilder("g1", "Gas_centrifuge",10).
addFluidInput(<liquid:HF>*10).
addEnergyperTickInput(500).
addFluidOuput(<liquid:dt_4>*8).
bulid();
RecipeBuilder.newBuilder("g2", "Gas_centrifuge",10).
addFluidInput(<liquid:dt_4>*10).
addEnergyperTickInput(500).
addFluidOuput(<liquid:dt_3>*8).
bulid();
RecipeBuilder.newBuilder("g3", "Gas_centrifuge",10).
addFluidInput(<liquid:dt_2>*10).
addEnergyperTickInput(500).
addFluidOuput(<liquid:dt_1>*8).
bulid();
RecipeBuilder.newBuilder("g4", "Gas_centrifuge",10).
addFluidInput(<liquid:dt_1>*10).
addEnergyperTickInput(500).
addFluidOuput(<liquid:dt>*8).
bulid();
##将HF通过气体离心机进行纯化，需要4次，最终得到高纯氘剂（四次纯化后由HF加工为氘剂的产率为0.4096）

RecipeBuilder.newBuilder("f1", "Fusion_tank",100).
addFuidInput(<liquid:dt>*10).
addEnergyPerTickOutput(100000).
addItemOutput(<contenttweaker:shinystar_matter>*1).setChance(0.1).
bulid();
##消耗DT发电y,有概率产出闪星物质

RecipeBuilder.newBuilder("r1", "energy_dismantler",3600).
addItemInput(<contenttweaker:shinystar_matter>*1).
addEnergyPerTickOutput(500).
addItemOutput(<contenttweaker:nanometer_material>*1).setChance(0.5).
bulid();
